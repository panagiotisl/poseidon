<% provide(:title, @agent.name) %>
<div class="row">
    <div class="span12">
        <%= render "/shared/nav" %>
        <div class="row">
            <div class="span3">
                <%= render "shared/left_menu" %>
            </div>
            <div class="span3 roundBox">
                <% @voyages_ports = @agent.voyages_ports.paginate(page: params[:page]) %>
                <h4>Voyages</h4>
                <%= will_paginate @voyages_ports %>
                <ul class="voyages">
                    <%= render @voyages_ports %>
                </ul>
                <%= will_paginate @voyages_ports %>
            </div>
        </div>
    </div>
</div>
