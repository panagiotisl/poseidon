<% if signed_in? %>
<div id="latest_doc">
    <%= render "shared/latest_feed" %>
</div>
<div id="conversation">
</div>
<% else %>
<div id="myCarousel" class="carousel slide">
    <!-- Carousel items -->
    <div class="carousel-inner">
        <div class="active item">
            <img src="http://placehold.it/1200x400&amp;text=Fade+effect">
        </div>
        <div class="item">
            <img src="http://placehold.it/1200x400&amp;text=Fade+effect">
        </div>
        <div class="item">
            <img src="http://placehold.it/1200x400&amp;text=Fade+effect">
        </div>
    </div><!-- Carousel nav --><a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
    <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
</div>
<script>
    $('.carousel').carousel({
        interval: 8000
    })
</script>
<div class="center hero-unit">
    <h1>Welcome to POSEIDON</h1>
    <h2>You can register a shipping company or a supplier by following
        one of the links below.</h2>
    <%= link_to "New Shipping Company", new_shipping_company_path,
    class: "btn btn-large btn-primary" %>
    <%= link_to "New Supplier", new_agent_path,
    class: "btn btn-large btn-primary" %>
</div>
<% end %>