<% provide(:title, @title) %>
<div class="row">
  <div class="span12">
    <%= render "nav" %>
    <div class="span5 roundBox">
      <h4>Ships</h4>
      <%= will_paginate @ships %>
      <ul class="ships">
        <%= render @ships %>
      </ul>
      <%= will_paginate @ships %>
    </div>
    <div class="span5 roundBox">
      <h4>Voyages</h4>
      <%= will_paginate @voyages %>
      <ul class="voyages">
        <%= render @voyages %>
      </ul>
      <%= will_paginate @voyages %>
    </div>
  </div>
</div>